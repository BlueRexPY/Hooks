{
  "compilerOptions": {
    "module": "es6",
    "target": "ESNext",
    "jsx": "react",
    "lib": [
      "dom",
      "esnext",
    ],
    "sourceMap": true,
    "rootDir": ".",
    "outDir": "build",
    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedParameters": true,
  }
